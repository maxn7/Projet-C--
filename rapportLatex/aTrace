
set terminal pdf
#set key off
a=2 #regle epesseur des traces
set key right outside #posittionne la legende
set xlabel "Temps"
set ylabel "V"

set output "images/C2triangle.pdf"
plot "../plot/C2triangle.dat" using 1:2 title "E(t)" with lines linewidth a,"../plot/C2triangle.dat" using 1:3 title "Vs(t)" with lines linewidth a ,"../plot/C2triangle.dat" using 1:4 title "Vd(t)" with lines linewidth a
unset output


set terminal latex
#set key off
set key right outside
set output "C2echelon.tex"
set xlabel "Temps"
set ylabel "V"
plot "../plot/C2echelon.dat"  using 1:2 with lines title "E(t)","../plot/C2echelon.dat" using 1:3 with lines title "Vs(t)","../plot/C2echelon.dat" using 1:4 with lines title "Vd(t)"
unset output

